ght: 100;\r\n}\r\n\r\n/******************************/\r\n/*****      BUTTONS      *****/\r\n/****************************/\r\n\r\n.btn-primary {\r\n  border-radius: 5px;\r\n  color: $lighten-text;\r\n  background: transparent;\r\n  border-width: 2px;\r\n  border-color: transparentize($primary, 0);\r\n  transition: all 0.38s ease-in;\r\n  &:hover {\r\n    color: $darken-text;\r\n    background: rgba($primary, 1);\r\n    border-color: transparentize($primary, 1);\r\n  }\r\n  &:not(:disabled):not(.disabled):active {\r\n    color: $darken-text;\r\n    background: rgba($primary, 1);\r\n    border-color: transparentize($primary, 1);\r\n  }\r\n  &:focus {\r\n    box-shadow: none;\r\n  }\r\n}\r\n\r\n/******************************/\r\n/*****   NAVBAR STYLES   *****/\r\n/****************************/\r\n\r\nnav {\r\n  .container-fluid {\r\n    padding: 0px 70px;\r\n  }\r\n  img {\r\n    width: 15%;\r\n  }\r\n}\r\n\r\n.sticky-top {\r\n  padding: 20px 10px;\r\n  background: transparentize($menubackground, 0.7);\r\n  // background: $menubackground;\r\n  transition: all 0.25s ease-in;\r\n}\r\n\r\n.stuck .sticky-top {\r\n  background: transparentize($menubackground, 0.8);\r\n  // color: darken($menubackground, 80%);\r\n  padding: 20px 10px;\r\n}\r\n\r\n/******************************/\r\n/*****   INTRO SECTION   *****/\r\n/****************************/\r\n\r\n.intro-section {\r\n  .header.bg {\r\n    height: 90vh;\r\n    background: $background;\r\n    // background-image: url('https://visualhunt.com/photos/xl/7/wood-cottage-wood-house-cabine.jpg');\r\n    // background-size: cover;\r\n    // background-position: center;\r\n  }\r\n}\r\n\r\n/******************************************/\r\n/*****   FEATURES APPROACH SECTION   *****/\r\n/****************************************/\r\n\r\n.features-approach {\r\n  .header.bg {\r\n    height: 60vh;\r\n    background: $background;\r\n  }\r\n  .row.title {\r\n    margin-bottom: 80px;\r\n  }\r\n  .row.items {\r\n    width: 80%;\r\n    .feature-icon {\r\n      svg {\r\n        width: 150px;\r\n        g {\r\n          fill: $primary;\r\n        }\r\n      }\r\n    }\r\n    p {\r\n      margin-top: 30px;\r\n      line-height: 1.7;\r\n    }\r\n  }\r\n}\r\n\r\n/******************************************/\r\n/*****    TABS FEATURES SECTION      *****/\r\n/****************************************/\r\n.tabs-features {\r\n  .header.bg {\r\n    // height: 60vh;\r\n    background: $background;\r\n  }\r\n  .row.title {\r\n    margin-bottom: 80px;\r\n  }\r\n  .row.items {\r\n    width: 90%;\r\n    ul#tabs {\r\n        list-style-type: none;\r\n      }\r\n      \r\n      ul#tabs li {\r\n        display: inline-block;\r\n        padding-top: 18px;\r\n        width: 33%;\r\n        opacity: 0.5;\r\n        border: 0;\r\n        border-radius: 0;\r\n        border-top: 1.5px solid transparentize($lighten-text, 0.8);\r\n        color: $lighten-text;\r\n        transition: all 0.38s ease-in;\r\n      }\r\n      \r\n      ul#tabs li.selected {\r\n        opacity: 1;\r\n        color: $lighten-text;\r\n        background-color: transparent;\r\n        border-color: $primary;\r\n      }\r\n      \r\n      ul#tabs li:hover {\r\n        opacity: 1;\r\n      }\r\n      \r\n      ul#tabs li a {\r\n        color: $lighten-text;\r\n        text-decoration: none;\r\n        font-size: 1em;\r\n        text-transform: capitalize;\r\n        font-weight: bold;\r\n      \r\n        p {\r\n          font-size: 0.9em;\r\n          font-weight: 200;\r\n        }\r\n      }\r\n      \r\n      ul#tabs li a:hover {\r\n        opacity: 1;\r\n      }\r\n      \r\n      div.tabContent {\r\n        display: none;\r\n      }\r\n  }\r\n}\r\n\r\n/******************************/\r\n/*****   FEATURES ITEMS   ****/\r\n/****************************/\r\n\r\n.features-items {\r\n    .header.bg {\r\n      // height: 60vh;\r\n      background: $background;\r\n      padding: 150px 0px;\r\n    }\r\n    .row.title {\r\n      margin-bottom: 80px;\r\n    }\r\n    .row.items {\r\n      width: 80%;\r\n      .feature-icon {\r\n        svg {\r\n          width: 150px;\r\n          g {\r\n            fill: $primary;\r\n          }\r\n        }\r\n      }\r\n      p {\r\n        margin-top: 30px;\r\n        line-height: 1.7;\r\n      }\r\n    }\r\n  }\r\n\r\n#video {\r\n  position: fixed;\r\n}\r\n\r\n#long {\r\n  height: 20000px;\r\n}\r\n","assets\\scss\\abstracts\\_variables.scss":"/***********************/\r\n/*****   Colors   *****/\r\n/***********************/\r\n\r\n$background: #000;\r\n$menubackground: #000;\r\n$primary: #1BCABD;\r\n$darken-text: #000; \r\n$lighten-text: #fff; \r\n\r\n\r\n\r\n$font-stack: 'Nunito', sans-serif;\r\n\r\n"},"lineCount":null}},"error":null,"hash":"0b6ff90ca5fc59147d4bad450fda4b37","cacheData":{"env":{}}}